<p id="notice"><%= notice %></p>

<h1>Paula and Jamie's Many Non-Linear Deals 🌻🐝</h1>

<br>

<div class="container">
  <div class="row">
    <% @deals.each do |deal| %>
      <div class="col-lg-3 col-md-3 col-sm-3 col-xs-4" style="padding:2px;">
        <a href="/deals/<%= deal.id %>">
        <img class="img-responsive" src="<%= deal.photo.url(:square)%>">
        </img>
      </a>
      </div>
    <% end %>
  </div>
</div>

<br>

<%= link_to 'New Deal', new_deal_path %>
